{
  "name": "nativescript-platform-css",
  "version": "1.6.1",
  "description": "A NativeScript plugin to deal with Declarative UI and Platform CSS",
  "main": "platformcss",
  "nativescript": {
	"platforms": {
		"android": "1.6.0",
		"ios": "1.6.0"
	},
	"plugin": {
	  "nan": "false",
	  "pan": "true",
	  "core3": "true",
	  "category": "Processing"
	}
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nathanaela/nativescript-platform-css.git"
  },
  "dependencies": {
    "nativescript-globalevents": "^1.2.1",
    "nativescript-platform": "^1.2.1"
  },
  "keywords": [
    "NativeScript", "platform", "css", "ios", "android"
